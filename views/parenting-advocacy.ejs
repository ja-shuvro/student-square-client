      <div class="our-bord-section">
        <div class="o-bord-sec">
          <h1><span class="one">Our</span> <span class="two">Board</span></h1>
          <% posts.forEach((post, index) => { %>
            <% if (post.designation.toLowerCase() === 'ceo') { %>
              <div class="big-card">
                <a href="/employee/<%= post.id %>" class="main-card">
                  <img
                    src="https://studentsquare.org/ss-admin<%= displayPicture[index].img.replace('public','') %>"
                    alt="<%=post.name%>"
                    srcset=""
                  />
                  <h3><%=post.name%></h3>
                  <h4><%=post.designation%></h4>
                  <h4><%=post.education%></h4>
                  <p>
                    <%= post.about.length <= 300 ? post.about : post.about.slice(0, 600) + '...' %>
                  </p>
                </a>
              </div>
            <% } %>
          <% }); %>

          <div class="small-card">
            <div class="row">

              <% posts.forEach((post, index) => { %>
              <div class="col-md-6">
                <a href="/employee/<%= post.id %>" class="main-s-card">
                  <img
                    src="https://studentsquare.org/ss-admin<%= displayPicture[index].img.replace('public','') %>"
                    alt="<%=post.name%>"
                    srcset=""
                  />
                  <h3><%=post.name%></h3>
                  <h4><%=post.designation%></h4>
                  <h4><%=post.education%></h4>
                  <p>
                    <%= post.about.length <= 300 ? post.about : post.about.slice(0, 600) + '...' %>
                  </p>
                </a>
              </div>
              <% }); %>
            </div>
          </div>
        </div>
