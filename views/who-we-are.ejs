                id="oparations-tab"
                role="tabpanel"
                aria-labelledby="oparations-tabs"
              >
                <div class="tab-cards">
                  <div class="row">
                    <% operations.forEach((post, index) => { %>
                    <div class="col-md-4">
                      <a href="/" class="main-tabs-card">
