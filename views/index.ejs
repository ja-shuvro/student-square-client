                <div class="slider-item s6" data-href="#" data-target="_blank">
                  <img src="/src/img/s-9/1.jpg" alt="" srcset="" />
                </div>
              </div>
            </div>
            <p>
              Student counseling is a process that involves providing guidance,
              support, and assistance to students in various aspects of their
              lives.
            </p>
          </div>
        </div>
      </div>
    </div>
    <!-- Section Ten  -->
    <div class="section-ten-main">
      <div class="section-ten">
        <h4>Student Square in the news</h4>

        <div class="news-slider">
          <div class="main-n-slider">
            <% posts.forEach((post, index) => { %>
            <div class="main-slider">
              <img
                src="https://studentsquare.org/ss-admin<%= thumbs[index].img.replace('public','') %>"
                alt="<%=post.title%>"
                srcset=""
              />
              <div class="dis">
                <img
                  src="https://studentsquare.org/ss-admin<%= logo[index].img.replace('public','') %>"
                  alt="<%=post.title%>"
                />
                <p class="newsTitle">
                  <a target="_blank" href="<%= post.link %>">
                    <%=post.title%>
                  </a>
                </p>
                <p class="card-text">
                  <% var discription = post.dis; var maxLength = 200; if
                  (discription.length > maxLength) { var truncatedText =
                  discription.substring(0, maxLength); while
                  (truncatedText.length > 0 &&
                  truncatedText.charCodeAt(truncatedText.length - 1) > 255) {
                  maxLength--; truncatedText = discription.substring(0,
                  maxLength); } discription = truncatedText + '...'; } %> <%=
                  discription %>
                </p>
              </div>
            </div>
            <% }) %>
          </div>

          <div class="subSlider">
            <div class="row sub-news-slider">
              <% posts.forEach((post, index) => { %>
              <div class="col-md-4">
                <div class="slider-main-s">
                  <img
                    class="logo"
                    src="https://studentsquare.org/ss-admin<%= logo[index].img.replace('public','') %>"
                    alt=""
                    srcset=""
                  />
                  <img
                    class="thumb"
                    src="https://studentsquare.org/ss-admin<%= thumbs[index].img.replace('public','') %>"
                    alt="<%=post.title%>"
                    srcset=""
                  />
                  <a target="_blank" href="<%= post.link %>">
                    <%=post.title%>
                  </a>
                  <p class="subTitle"><%=post.dis%></p>
                </div>
              </div>
              <% }) %>
            </div>
          </div>
        </div>
      </div>

      <div class="section-ten-two">
        <img src="/src/img/SSPortraitLogo.png" alt="" srcset="" />
        <h3>Our Partners</h3>
        <div class="partner-box">
          <div class="row">
            <% partners.allPost.forEach((post, index) => { %>
            <div class="col-md-3 mb-2">
              <div
                class="p-main-box"
                style="
                  display: flex;
                  align-items: center;
                  justify-content: center;
                  flex-direction: column;
                "
              >
                <img
                  src="https://studentsquare.org/ss-admin<%= partners.logo[index].img.replace('public','') %>"
                  alt="<%=post.title%>"
                  srcset=""
                  style="
                    padding: 0;
                    margin: 0;
                    width: 100px;
                    height: auto;
                    object-fit: contain;
                    border-radius: 0;
                  "
                />
                <p
                  style="
                    margin: 0;
                    margin-top: 2px;
                    color: #000;
                    font-weight: 600;
                  "
                >
                  <%=post.title%>
                </p>
              </div>
            </div>
            <% }); %>
          </div>
        </div>
      </div>
    </div>

